{{#each quizset}}

  <div class="card">
  <div class="cardheader">
  <p><b> {{name}} </b></p>
  </div>
  <div class="container"> 
    <p class="versionb{{index}}"> 
    <b>{{num_question}}</b> Questions in set. <br><br> <b>Preview:</b> 
    {{#if this.questions.[0]}} <br> {{this.questions.[0].q_text}} {{else}} No questions in set, <br> please click to add : ) {{/if}} 
    {{#if this.questions.[1]}} <br> {{this.questions.[1].q_text}} {{/if}}  
    {{#if this.questions.[2]}} <br> {{this.questions.[2].q_text}} {{/if}} 
    {{#if this.questions.[3]}} <br>  
     <div class="schedulebutton previewbtn">
      <p><a id="pre{{index}}" href="javascript:void(0);" onclick="preview{{index}}()"><b> View More  </b></a></p>
    </div>  
    {{/if}}
